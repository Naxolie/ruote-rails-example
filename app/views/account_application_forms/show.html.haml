- title "Account Application Form"

%p
  %strong Name:
  =h @account_application_form.name
%p
  %strong Account Application Form:
  = image_tag @account_application_form.attachment.url if @account_application_form.attachment
  

%p
  = link_to "Edit", edit_account_application_form_path(@account_application_form)
  |
  = link_to "Destroy", @account_application_form, :confirm => 'Are you sure?', :method => :delete
  |
  = link_to "View All", account_application_forms_path
